{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "API Exercita",
      "description": "Documentação da API do projeto Exercita."
    },
    "host": "localhost:3000",
    "basePath": "/",
    "schemes": ["http"],
    "paths": {
      "/autentic/login": {
        "post": {
          "summary": "Autentica o usuário",
          "tags": ["Autenticação"],
          "parameters": [
            {
              "in": "body",
              "name": "login",
              "description": "Credenciais de login do usuário",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Login"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Usuário autenticado com sucesso"
            },
            "401": {
              "description": "Credenciais inválidas"
            }
          }
        }
      },
      "/autentic/cadastroUsuario": {
        "post": {
          "summary": "Cadastra um novo usuário",
          "tags": ["Autenticação"],
          "parameters": [
            {
              "in": "body",
              "name": "usuario",
              "description": "Informações para cadastrar o usuário",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Usuario"
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Usuário cadastrado com sucesso"
            }
          }
        }
      },
      "/locais/cadastro-local": {
        "post": {
          "summary": "Cadastra um novo local",
          "tags": ["Locais"],
          "parameters": [
            {
              "in": "body",
              "name": "local",
              "description": "Dados do local a ser cadastrado",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Local"
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Local cadastrado com sucesso"
            }
          }
        }
      },
      "/locais/todos-locais": {
        "get": {
          "summary": "Retorna todos os locais",
          "tags": ["Locais"],
          "responses": {
            "200": {
              "description": "Lista de locais",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/Local"
                }
              }
            }
          }
        }
      },
      "/usuarios": {
        "get": {
          "summary": "Retorna todos os usuários",
          "tags": ["Usuários"],
          "responses": {
            "200": {
              "description": "Lista de usuários",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/Usuario"
                }
              }
            }
          }
        }
      },
      "/usuarios/{id}": {
        "put": {
          "summary": "Atualiza um usuário",
          "tags": ["Usuários"],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "description": "ID do usuário a ser atualizado",
              "type": "integer"
            },
            {
              "in": "body",
              "name": "usuario",
              "description": "Dados atualizados do usuário",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Usuario"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Usuário atualizado com sucesso"
            }
          }
        },
        "delete": {
          "summary": "Deleta um usuário",
          "tags": ["Usuários"],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "description": "ID do usuário a ser deletado",
              "type": "integer"
            }
          ],
          "responses": {
            "200": {
              "description": "Usuário deletado com sucesso"
            }
          }
        }
      }
    },
    "definitions": {
      "Usuario": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "nome": {
            "type": "string"
          },
          "sexo": {
            "type": "string"
          },
          "cpf": {
            "type": "string"
          },
          "endereco": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password_hash": {
            "type": "string"
          },
          "data_nacimento": {
            "type": "string"
          },
          "isAdmin": {
            "type": "boolean"
          }
        }
      },
      "Local": {
        "type": "object",
        "properties": {
          "idLocal": {
            "type": "integer"
          },
          "nomeLocal": {
            "type": "string"
          },
          "descricaoLocal": {
            "type": "string"
          },
          "rua_endereco": {
            "type": "string"
          },
          "numero_endereco": {
            "type": "string"
          },
          "bairro_endereco": {
            "type": "string"
          },
          "cidade_endereco": {
            "type": "string"
          },
          "estado_endereco": {
            "type": "string"
          },
          "cep_endereco": {
            "type": "string"
          },
          "complemento_endereco": {
            "type": "string"
          },
          "horario_funcionamento": {
            "type": "string"
          },
          "latitude": {
            "type": "string"
          },
          "longitude": {
            "type": "string"
          },
          "google_maps_link": {
            "type": "string"
          },
          "idUsuario": {
            "type": "integer"
          }
        }
      },
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    }
  }
  